# AWS Configuration
aws_region = "us-east-1"

# Domain Configuration
domain_name  = "example.com"
www_redirect = true  # true = www -> apex, false = apex -> www

# CloudFront Configuration
cloudfront_price_class = "PriceClass_100"  # Options: PriceClass_100, PriceClass_200, PriceClass_All

# Logging Configuration
log_retention_days = 90

# Alarm Thresholds
error_4xx_threshold = 10  # Percentage
error_5xx_threshold = 5   # Percentage